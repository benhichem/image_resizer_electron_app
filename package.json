  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/node": "^22.6.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "electron": "^28.1.0",
    "eslint": "^7.32.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@types/resize-img": "^2.0.3",
    "@types/toastify-js": "^1.12.3",
    "resize-img": "^2.0.0",
    "toastify-js": "^1.12.0"
  }
